<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.app.com/apk/res/app"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <font
        app:fontStyle="normal"
        app:fontWeight="100"
        app:font="@font/thin"
        android:font="@font/thin"
        android:fontStyle="normal"
        android:fontWeight="100"/>
    <font
        app:fontStyle="normal"
        app:fontWeight="200"
        app:font="@font/light"
        android:font="@font/light"
        android:fontStyle="normal"
        android:fontWeight="200"/>
    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/regular"
        android:font="@font/regular"
        android:fontStyle="normal"
        android:fontWeight="400"/>
    <font
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/semi_bold"
        android:font="@font/semi_bold"
        android:fontStyle="normal"
        android:fontWeight="600"/>
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/bold"
        android:font="@font/bold"
        android:fontStyle="normal"
        android:fontWeight="700"/>


</font-family>